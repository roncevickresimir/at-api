{
    "bracketSpacing": true,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "quoteProps": "as-needed",
    "arrowParens": "always",
    "parser": "typescript",
    "requirePragma": false,
    "endOfLine": "lf",
    "importOrder": ["<THIRD_PARTY_MODULES>", "^@(.*)$", "^[../]", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
  